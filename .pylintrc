[FORMAT]
indent-string='  '
max-line-length=80

[BASIC]
function-naming-style=camelCase
argument-naming-style=camelCase
variable-naming-style=camelCase

[DESIGN]
max-attributes=15

[MESSAGES CONTROL]
disable-checker=wrong-hanging-indentation,invalid-name,indent-after-paren,bad-continuation
disable=bad-continuation,bad-whitespace,import-error,ungrouped-imports,missing-docstring,invalid-name,multiple-imports,unidiomatic-typecheck,line-too-long,unused-variable,too-few-public-methods,bare-except,trailing-whitespace,too-many-locals,too-many-branches,too-many-statements,unused-argument,fixme,redefined-builtin,consider-using-ternary,no-else-return,superfluous-parens,too-many-arguments,old-style-class,bad-indentation,wrong-import-order,unused-import,logging-not-lazy,anomalous-backslash-in-string,no-self-use,trailing-newlines,expression-not-assigned,pointless-statement,inconsistent-return-statements,attribute-defined-outside-init,broad-except, deprecated-lambda, duplicate-code, len-as-condition, multiple-statements, no-init, no-value-for-parameter, protected-access,redefined-outer-name, singleton-comparison,super-init-not-called,too-many-lines,too-many-nested-blocks,too-many-return-statements,unneeded-not,too-many-ancestors,deprecated-method,useless-import-alias,useless-object-inheritance,consider-using-set-comprehension,undefined-variable,useless-import-alias,too-many-ancestors,consider-using-dict-comprehension,invalid-envvar-default,logging-format-interpolation
